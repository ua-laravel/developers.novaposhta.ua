{
    "name": "profiwm/novaposhta",
    "description": "NovaPoshta (API 2.0) for Laravel",
    "keywords": [
        "profiwm",
        "novaposhta",
        "ukraine-delivery",
        "laravel-np",
        "laravel-novaposhta",
        "delivery"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ua.lifesheets",
            "email": "ua.lifesheets@icloud.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/framework": "^9|^10",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "ProfiWM\\NovaPoshta\\Facades\\": "src/Facades",
            "ProfiWM\\NovaPoshta\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProfiWM\\NovaPoshta\\NovaPoshtaServiceProvider"
            ],
            "aliases": {
                "NovaPoshta": "ProfiWM\\NovaPoshta\\Facades\\NovaPoshta"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}